{
  "name": "react-attribute-remover",
  "version": "1.0.0",
  "description": "A Vite plugin for removing specified attributes from React/JSX components at build time",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-attribute-remover.git"
  },
  "homepage": "https://github.com/yourusername/react-attribute-remover#readme",
  "bugs": {
    "url": "https://github.com/yourusername/react-attribute-remover/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "rollup -c --watch"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "react",
    "jsx",
    "attributes",
    "remove",
    "strip",
    "data-testid",
    "data-cy",
    "build",
    "optimization",
    "production"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "rollup": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  }
}
